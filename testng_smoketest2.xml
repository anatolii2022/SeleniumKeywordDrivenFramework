<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite Linux" verbose="1" parallel="tests" thread-count="2">

	<test name="Test Windows10">
		<groups>
			<run>
				<include name="windows"></include>
			</run>
		</groups>
		<classes>
			<class name="com.freecrm.smoketests.SmokeTest2Linux" />
		</classes>
	</test> <!-- Test Linux -->

	<test name="Test Linux Ubuntu">
		<groups>
			<run>
				<include name="linux"></include>
			</run>
		</groups>
		<classes>
			<class name="com.freecrm.smoketests.SmokeTest2Linux"></class>
		</classes>
	</test>
</suite> <!-- Suite -->
